{
	"version": "0.2.0",
	"configurations": [
		{
			"request": "launch",
			"name": "Server",
			"type": "node",
			"program": "${workspaceFolder}/server/main.ts",
			"cwd": "${workspaceFolder}/server",
			"runtimeExecutable": "tsx",
			"runtimeArgs": [
				"--env-file=${workspaceFolder}/server/.env"
			],
			"console": "integratedTerminal"
		},
		{
			"request": "launch",
			"name": "Manage",
			"type": "node",
			"program": "${workspaceFolder}/scripts/manage.ts",
			"cwd": "${workspaceFolder}/scripts",
			"runtimeExecutable": "tsx",
			"runtimeArgs": [
				"--env-file=${workspaceFolder}/scripts/.env"
			],
			"console": "integratedTerminal"
		}
	]
}